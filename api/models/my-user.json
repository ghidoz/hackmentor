{
  "name": "MyUser",
  "base": "User",
  "idInjection": true,
  "mixins": {
    "EnhancedContext": {}
  },
  "options": {
    "realmRequired": false,
    "validateUpsert": true
  },
  "properties": {
    "firstName": {
      "type": "string"
    },
    "familyName": {
      "type": "string"
    }
  },
  "scope": {
      "include": [
        "roles"
      ]
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "through": "RoleMapping",
      "keyThrough": "roleId",
      "foreignKey": "principalId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
