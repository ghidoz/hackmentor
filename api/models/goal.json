{
  "name": "Goal",
  "idInjection": true,
  "mixins": {
    "EnhancedContext": {}
  },
  "options": {
    "validateUpsert": true
  },
  "properties": {

    "hoursPerWeek": {
      "type": "number",
      "required": true
    },
    "weeklyBudget": {
      "type": "number",
      "required": true
    },
    "headline": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    }
  },
  "scope": {
  },
  "validations": [],
  "relations": {
    "apprentice": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "apprenticeId"
    },
    "mentor": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "mentorId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
